                </mxCell>
            </CatchSignalEvent>
        </add>
        <add as="CatchMessageEvent">
            <CatchMessageEvent label="Intermediate message catching event" type="catchingEvent">
                <mxCell vertex="true" style="roundImage;image=mxgraph/images/catching/message.png">
                    <mxGeometry as="geometry" x="0" y="0" width="40" height="40"/>
                </mxCell>
            </CatchMessageEvent>
        </add>
        <add as="ThrowNoneEvent">
            <ThrowNoneEvent label="Intermediate none throwing event" type="throwingEvent">
                <mxCell vertex="true" style="roundImage;image=mxgraph/images/throwing/none.png">
                    <mxGeometry as="geometry" x="0" y="0" width="40" height="40"/>
                </mxCell>
            </ThrowNoneEvent>
        </add>
        <add as="ThrowSignalEvent">
            <ThrowSignalEvent label="Intermediate signal throwing event" type="throwingEvent">
                <mxCell vertex="true" style="roundImage;image=mxgraph/images/throwing/signal.png">
                    <mxGeometry as="geometry" x="0" y="0" width="40" height="40"/>
                </mxCell>
            </ThrowSignalEvent>
        </add>
        
        <add as="EndNoneEvent">
            <EndNoneEvent label="End event" type="endEvent">
                <mxCell vertex="true" style="roundImage;image=mxgraph/images/endevent/event_end.png">
                    <mxGeometry as="geometry" x="0" y="0" width="40" height="40"/>
                </mxCell>
            </EndNoneEvent>
        </add>
        <add as="EndErrorEvent">
            <EndErrorEvent label="End error event" type="endEvent">
                <mxCell vertex="true" style="roundImage;image=mxgraph/images/startevent/none.node.png">
                    <mxGeometry as="geometry" x="0" y="0" width="40" height="40"/>
                </mxCell>
            </EndErrorEvent>
        </add>
        <add as="SequenceFlow">
            <SequenceFlow label="flow" type="edge">
                <mxCell edge="true" style="defaultEdge">
                    <mxGeometry relative="true" as="geometry" x="0" y="0"/>
                </mxCell>
            </SequenceFlow>
        </add>
        
    </Array>
</mxEditor>